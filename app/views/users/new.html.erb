<div id="user-new">
  <section class="content user-list-block-1">
    <div class="new-form">
      <div class="table-responsive">
        <fieldset>
          <div class="form-group col-md-7 has-error">
            <label for="inputName" class="col-md-2 control-label">Name<span></span></label>
            <div class="col-md-7">
              <input type="text" class="form-control" id="inputName" placeholder="Name" required autofocus="true">
